package com.zoom.pages;

import org.openqa.selenium.WebElement;

import com.zoom.cons.YamlReader;

public class User {
	//建YamlReader例
	private YamlReader yaml; 
	//目菜谓M件
	private WebElement users, pending, advanced;
	//searchM件
	private WebElement ssoonly, searchemail, usertype, domain, search;
	//user list
	private WebElement checkall, selection, edit, delete, pages, total, pagesize;
	//user action
	private WebElement addnew, importuser, exportuser, setadmin, batchedit, assign;
	//advanced
	private WebElement totaluser, changefrom, changeto, change, removetype, remove, defaultuser, save, 
		usergroupfrom, usergroupto, changegroup, document;
	//造函党跏蓟M件
	public User(){
		pageheader = yaml.getElement("pageheader");
		check_msg = yaml.getElement("check_msg");
		email = yaml.getElement("email");
		send = yaml.getElement("send");
		pageheader2 = yaml.getElement("pageheader2");
		check_email = yaml.getElement("check_email");
		resend = yaml.getElement("resend");
	}

	//M件get函
	public YamlReader getYaml() {
		return yaml;
	}

	public WebElement getPageheader() {
		return pageheader;
	}

	public WebElement getCheck_msg() {
		return check_msg;
	}

	public WebElement getEmail() {
		return email;
	}

	public WebElement getSend() {
		return send;
	}

	public WebElement getPageheader2() {
		return pageheader2;
	}

	public WebElement getCheck_email() {
		return check_email;
	}

	public WebElement getResend() {
		return resend;
	}
	
	//M件基本y函
	
}
